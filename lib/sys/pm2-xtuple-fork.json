[{
  "name": "xt-{name}-{version}",
  "script": "/usr/sbin/{name}/{version}/main.js",
  "args": "[ '-c /etc/xtuple/{version}/{name}/config.js' ]",
  "env": {
    "NODE_ENV": "production",
    "XT_APPID": "xt-{name}-{version}",
    "XT_PORT": "{port}"
  },
  "port": "{port}",
  "exec_mode": "fork_mode",
  "out_file": "/var/log/xtuple/{version}/{name}/access.log",
  "error_file": "/var/log/xtuple/{version}/{name}/error.log",
  "pid_file": "/var/run/xtuple/{name}-{version}.pid",
  "min_uptime": 30000
}]
